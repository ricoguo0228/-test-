.we7-bottom {
    display: flex;
    overflow: hidden;
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    padding: 10rpx 0;
    border-top: 1rpx solid #fff;
}

.we7-bottom,.we7-bottom.active,.we7-bottom.active navigatorurl {
    background-color: #fff;
}

.we7-bottom .we7-bottom-item {
    flex: 1;
    padding: 5rpx;
    text-align: center;
}

.we7-bottom .item-img {
    width: 40rpx;
    height: 40rpx;
    display: inline-block;
}

.we7-bottom .item-text {
    display: block;
    font-size: 12px;
    line-height: 1;
}

.container {
    font-size: 12px;
    justify-content: space-between;
    box-sizing: border-box;
}

.container,.modal {
    display: flex;
    flex-direction: column;
}

.modal {
    width: 100%;
    height: 100%;
    background: rgba(0,0,0,.5);
    position: fixed;
    left: 0;
    top: 0;
    z-index: 50000;
    align-items: center;
    justify-content: center;
    text-align: center;
    font-size: 28rpx;
}

.m_box {
    background: #fff;
    border-radius: 10rpx;
    width: 70%;
    padding: 20rpx 0 10rpx;
}

.border_top_2 {
    margin-top: 10rpx;
    padding-top: 10rpx;
    border-top: 2rpx solid #f3f3f3;
}

.m_btn {
    background: #fff;
    border: none;
    color: #199fb1;
    line-height: 2em;
}

.m_btn::after {
    border: none;
    width: 0;
    height: 0;
}

page {
    background: #fff;
}

.bgmain {
    background-color: #ff6444;
    color: #fff;
}

.contents {
    padding: 70rpx 0 30rpx;
    color: #fff;
    font-size: 30rpx;
    text-align: center;
    background: #ff6444;
}

.slide_box {
    position: relative;
    background-color: #fff;
}

.indicator-dots {
    width: 100%;
    position: absolute;
    bottom: 30rpx;
    left: 0;
    display: flex;
    justify-content: center;
}

.indicator-dots view {
    width: 12rpx;
    height: 12rpx;
    background: #d1d1d1;
    margin: 0 5rpx;
    border-radius: 50%;
}

.indicator-dots view.dots_on {
    background: #fff;
}

.cate_item_box {
    margin-right: 20rpx;
    overflow: scroll;
    height: 60rpx;
    width: 100%;
}

.cate_item {
    margin: 0 30rpx;
    font-size: 34rpx;
    color: #999;
    position: relative;
    flex-shrink: 0;
    height: 40rpx;
}

.cate_active {
    color: #000;
    font-weight: 700;
    font-size: 34rpx;
}

.cate_active::after {
    position: absolute;
    content: "";
    width: 60rpx;
    height: 2rpx;
    background: #ff6344;
    bottom: -10rpx;
    left: 0;
    right: 0;
    margin: 0 auto;
}

.label_box {
    flex-shrink: 0;
}

.label_item {
    border: 2rpx solid #ff6344;
    padding: 5rpx 15rpx;
    color: #ff6344;
}

.label_item_active {
    background: #ff6344;
    color: #fff;
}

.article_box {
    margin-top: 40rpx;
    box-shadow: 0 4px 20px 0 hsla(0,0%,60%,.1);
    border-radius: 10rpx;
}

.article_box .avatar {
    width: 75rpx;
    height: 75rpx;
    border-radius: 50%;
    margin-right: 20rpx;
}

.article_box .nickname {
    font-size: 30rpx;
    font-weight: 700;
}

.article_box .date {
    font-size: 24rpx;
    color: #999;
}

.icon_item image {
    width: 35rpx;
    height: 35rpx;
}

.icon_item text {
    margin-left: 10rpx;
    color: #999;
}

.btn {
    background: #ff6344;
    color: #fff;
    width: 70%;
    margin-left: 15%;
    padding: 30rpx;
    border-radius: 50rpx;
    position: fixed;
    bottom: 20rpx;
}

.post_img {
    width: 90rpx;
    height: 90rpx;
    position: fixed;
    right: 0;
    bottom: 32%;
    background: #00bb69;
    border-radius: 50%;
    padding: 20rpx;
}

.item_img {
    width: 180rpx;
    height: 180rpx;
    margin-right: 20rpx;
}

.my_fixed {
    width: 90rpx;
    height: 90rpx;
    position: fixed;
    right: 0;
    bottom: 20%;
    background: #ff6344;
    border-radius: 50%;
    line-height: 90rpx;
    text-align: center;
    color: #fff;
}

.publish {
    position: fixed;
    bottom: -110rpx;
    left: 50%;
    transform: translateX(-50%);
}

.publish_item {
    background: #ff6444;
    width: 220rpx;
    height: 220rpx;
    border-radius: 50%;
}

.my {
    width: 50rpx!important;
    height: 50rpx!important;
    margin: 10rpx 0 5rpx;
}

.publish image {
    width: 65rpx;
    height: 65rpx;
}

.item {
    font-size: 28rpx;
    margin: 10rpx 0;
}

.search_box {
    height: 70rpx;
    width: 90%;
    border-radius: 50rpx;
    margin: 10rpx auto 50rpx;
    background: #f2f2f2;
    text-align: center;
    line-height: 70rpx;
    font-size: 28rpx;
    color: grey;
}